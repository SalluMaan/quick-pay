{
  "expo": {
    "name": "Quicki Pay",
    "slug": "quick-pay",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.maan_sallu786.quickpay"
    },
    "androidStatusBar": {
      "backgroundColor": "#000"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
